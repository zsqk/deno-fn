{
  "name": "@zsqk/somefn",
  "version": "0.0.2",
  "exports": "./mod.ts",
  "publish": {
    "include": ["js", "deno", "ts"],
    "exclude": ["**/*.test.ts", "**/*.benchmark.ts"]
  },
  "compilerOptions": {
    "strict": true
  },
  "lint": {
    "files": {
      "include": [
        "js/",
        "ts/",
        "deno/",
        "test/",
        "mod.ts"
      ]
    },
    "rules": {
      "tags": [
        "recommended"
      ],
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "no-throw-literal"
      ],
      "exclude": [
        "ban-unused-ignore"
      ]
    }
  },
  "fmt": {
    "include": [
      "js/",
      "ts/",
      "deno/",
      "test/",
      "mod.ts"
    ],
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true
  },
  "imports": {
    "@std/async": "jsr:@std/async@^0.224.1",
    "@std/encoding": "jsr:@std/encoding@^0.224.3",
    "@std/fs": "jsr:@std/fs@^0.229.1",
    "@std/testing": "jsr:@std/testing@0.217.0",
    "@std/assert": "jsr:@std/assert@0.217.0",
    "@std/asyn2": "jsr:@std/async@0.217.0",
    "hmac/": "https://deno.land/x/hmac@v2.0.1/",
    "@std/encodin2": "jsr:@std/encoding@0.217.0",
    "@std/cli": "jsr:@std/cli@0.217.0"
  }
}
